        <script>${nameNode}/user/maria_dev/casestudyoozie/Copydata.hql</script>
      </hive>
      <ok to="end"/>
      <error to="kill_job"/>
    </action>


    <kill name="kill_job">
      <message>Job failed</message>
    </kill>
    <end name="end"/> </workflow-app>
